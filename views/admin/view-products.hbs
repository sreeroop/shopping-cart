<section>
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">No</th>
                    <th scope="col">Name</th>
                    <th scope="col">Category</th>
                    <th scope="col">Description</th>
                    <th scope="col">image</th>
                </tr>
            </thead>
            {{#each products}}
            <tbody>
                <tr>
                    <th scope="row">1</th>
                    <td>{{this.name}}</td>
                    <td>{{this.category}}</td>
                    <td>{{this.desc}}</td>
                    <td>
                        <img style="width: 50px;height: 50px;" src="{{this.image}}">
                    </td>
                </tr>

            </tbody>
            {{/each}}
        </table>
    </div>
</section>